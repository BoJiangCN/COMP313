@model WeLinkUp.Models.CreateEvent

@{ ViewData["Title"] = "Event Details"; }

<html>
<body>
    <br />
    <br />

    <h1 class="w3-content">Event Details</h1>
    <hr />
    <h3>Event Title:    @Html.DisplayFor(model => model.EventTitle)</h3>
